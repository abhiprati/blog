<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>blog</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>data</servlet-name>
    <servlet-class>com.data.database</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>data</servlet-name>
    <url-pattern>/data</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>database</servlet-name>
    <servlet-class>login.logindata</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>database</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addpost</servlet-name>
    <servlet-class>com.addpost.addpost</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addpost</servlet-name>
    <url-pattern>/addpostservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loader</servlet-name>
    <servlet-class>com.load.loader</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loader</servlet-name>
    <url-pattern>/load</url-pattern>
  </servlet-mapping>
</web-app>